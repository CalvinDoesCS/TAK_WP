{
  "name": "laravel/laravel",
  "type": "project",
  "description": "The skeleton application for the Laravel framework.",
  "keywords": [
    "laravel",
    "framework"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.2",
    "ext-curl": "*",
    "ext-pdo": "*",
    "ext-zip": "*",
    "barryvdh/laravel-dompdf": "^3.0",
    "google/auth": "^1.49",
    "laravel/framework": "^12.0",
    "laravel/reverb": "^1.0",
    "laravel/sanctum": "^4.0",
    "laravel/telescope": "^5.2",
    "laravel/tinker": "^2.9",
    "maatwebsite/excel": "^3.1",
    "nwidart/laravel-modules": "^12.0",
    "openai-php/client": "^0.10.3",
    "owen-it/laravel-auditing": "^14.0",
    "razorpay/razorpay": "^2.9",
    "simplesoftwareio/simple-qrcode": "^4.2",
    "spatie/laravel-backup": "^9.0",
    "spatie/laravel-permission": "^6.9",
    "srmklive/paypal": "^3.0",
    "tailflow/laravel-orion": "^2.22",
    "taylanunutmaz/agora-token-builder": "^1.1",
    "tymon/jwt-auth": "^2.1",
    "yajra/laravel-datatables": "^12.0",
    "yajra/laravel-datatables-oracle": "^12.0"
  },
  "require-dev": {
    "fakerphp/faker": "^1.23",
    "larastan/larastan": "^3.8",
    "laravel/boost": "^1.8",
    "laravel/dusk": "^8.3",
    "laravel/pint": "^1.13",
    "laravel/sail": "^1.26",
    "mockery/mockery": "^1.6",
    "nunomaduro/collision": "^8.0",
    "phpunit/phpunit": "^11.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/",
      "Modules\\": "Modules/"
    },
    "files": [
      "app/Helpers/GlobalHelpers.php",
      "app/Helpers/CoreModuleHelpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    },
    "files": [
      "app/Helpers/Helpers.php",
      "app/Helpers/TimeHelper.php"
    ]
  },
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-update-cmd": [
      "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi",
      "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
      "@php artisan migrate --graceful --ansi"
    ],
    "analyse": "vendor/bin/phpstan analyse --memory-limit=2G",
    "analyse:baseline": "vendor/bin/phpstan analyse --generate-baseline --memory-limit=2G",
    "test": "php artisan test",
    "test:filter": "php artisan test --filter"
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    },
    "merge-plugin": {
      "include": [
        "Modules/*/composer.json"
      ]
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true,
      "php-http/discovery": true,
      "wikimedia/composer-merge-plugin": true
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
